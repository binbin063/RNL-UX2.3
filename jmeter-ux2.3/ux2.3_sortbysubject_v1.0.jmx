int userId = Integer.parseInt(vars.get(&quot;userId&quot;));
int userIdEnd = Integer.parseInt(vars.get(&quot;userIdEnd&quot;));
int attendeesMailId = userId + 1;
if (attendeesMailId &gt; userIdEnd) {
	attendeesMailId = userId - 1;
}

vars.put(&quot;eventLocalStart&quot;, format.format(now));
vars.put(&quot;eventLocalEnd&quot;, format.format(endTime));

