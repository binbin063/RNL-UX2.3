endCal.add(Calendar.HOUR_OF_DAY, 3); 

Date endTime = endCal.getTime();
//as &apos;YmdTHis&apos; format in JS, such as 20171107T171500
SimpleDateFormat format = new SimpleDateFormat(&quot;yyyyMMdd&apos;T&apos;HHmmss&quot;); 


