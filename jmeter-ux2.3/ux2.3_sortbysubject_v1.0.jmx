vars.put(&quot;userName&quot;, vars.get(&quot;userPrefix&quot;) + userId);
vars.put(&quot;user&quot;, vars.get(&quot;userPrefix&quot;) + userId + &quot;@&quot; + vars.get(&quot;userDomain&quot;));

